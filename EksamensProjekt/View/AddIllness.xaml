<Window x:Class="EksamensProjekt.View.AddIllness"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tilføj Sygdom" Height="135" Width="260" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Navn" FontSize="14px" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top"/>
        <TextBox Name="IllnessName_TextBox" MaxLength="100" HorizontalAlignment="Left" Height="25" Margin="68,25,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="165" PreviewTextInput="IllnessName_TextBox_PreviewTextInput" />
        <Button Name="AddIllness_Button" Content="Tilføj Sygdom" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top" Height="25" Width="100" Click="AddIllness_Button_Click"/>
        <Button Name="Cancel_Button" Content="Annuller" HorizontalAlignment="Left" Margin="133,59,0,0" VerticalAlignment="Top" Height="25" Width="100" Click="Cancel_Button_Click"/>

    </Grid>
</Window>
