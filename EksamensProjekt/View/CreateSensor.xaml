        <Label Content="Type" HorizontalAlignment="Left" Margin="22,69,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="Type_ComboBox" HorizontalAlignment="Left" Margin="121,69,0,0" VerticalAlignment="Top" Width="143" SelectionChanged="Type_ComboBox_SelectionChanged"/>
        <Button Name="AddSensor_Button" Content="Tilføj" HorizontalAlignment="Left" Margin="157,113,0,0" VerticalAlignment="Top" Width="102" RenderTransformOrigin="0,-0.364" Click="AddSensor_Button_Click"/>

    </Grid>
